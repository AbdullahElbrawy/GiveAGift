{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
     
    }
  ],
  "routes": [
    {
      "src": "/server/(.*)",
      "dest": "/server/$1.js"
    },
    {
      "src": "/(.*)",
      "headers": { "cache-control": "s-maxage=1, stale-while-revalidate" },
      "dest": "/build/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
